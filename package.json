		"@types/react-dom": "^19.1.2",
		"@vitejs/plugin-react": "^4.4.1",
		"classnames": "^2.5.1",
		"eslint": "^9.29.0",
		"eslint-plugin-prettier": "^5.5.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.2",
		"prettier": "^3.6.0",
		"prettier-plugin-tailwindcss": "^0.6.13",
		"saas": "^1.0.0",
