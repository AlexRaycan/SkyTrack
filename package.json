		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"flag-icons": "^7.5.0",
		"framer-motion": "^12.18.1",
		"leaflet": "^1.9.4",
		"lucide-react": "^0.525.0",
		"motion": "^12.19.2",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.11"
	},
	"devDependencies": {
		"@eslint/js": "^9.29.0",
		"@stylistic/eslint-plugin": "^5.0.0",
		"@svgr/plugin-svgo": "^8.1.0",
